  precompile static method powi : int ($base : int, $exponant : int) {
    
    unless ($exponant >= 0) {
      die "The \$exponant number must be greater than or equal to 0";
    }
    
    if ($base == 0) {
      unless ($exponant != 0) {
        die "If the \$base number is 0, the \$exponant number can't be 0";
      }
    }
    
    my $ret = 1;
    for (my $i = 0; $i < $exponant; $i++) {
      $ret = $ret * $base;
    }
    
    return $ret;
  }
  
