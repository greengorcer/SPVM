package TestCase::FatCamma {

  sub fat_camma : int () {
    my $kv = [(object)foo => 1, "bar" => 2];
    
    unless ((string)$kv->[0] eq "foo") {
      return 0;
    }

    unless ((int)$kv->[1] == 1) {
      return 0;
    }

    unless ((string)$kv->[2] eq "bar") {
      return 0;
    }

    unless ((int)$kv->[3] == 2) {
      return 0;
    }
    
    return 1;
  }
}
