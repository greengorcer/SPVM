class TestCase::Unless {
  static method unless_ok : int () {
    unless(0) {
      return 1;
    }
    
    return 0;
  }

  static method unless_not_ok : int ()  {
    unless(1) {
      return 0;
    }
    
    return 1;
  }
  
  static method unless_else : int ()  {
    unless (1) {
      return 0;
    }
    else {
      return 1;
    }
    
    return 0;
  }
}
