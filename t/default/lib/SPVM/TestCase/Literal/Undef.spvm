package TestCase::Literal::Undef {
  use TestCase::Minimal;
  
  sub undef_test : int () {
    my $minimal = TestCase::Minimal->new;
    my $minimal_undef : TestCase::Minimal;
    
    unless ($minimal != undef) {
      return 0;
    }
    
    unless ($minimal_undef == undef) {
      return 0;
    }
    
    unless (!undef) {
      return 0;
    }
    
    return 1;
  }
}
