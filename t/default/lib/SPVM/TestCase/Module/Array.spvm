      }
      unless ($array_out->[0] == $value0) {
        return 0;
      }
    }
    
    return 1;
  }
  
  static method copy_range_byte : int () {
    {
      my $array = [(byte)5, -7, 9, Fn->BYTE_MIN(), 127, 15, 19];
      my $range = Array->copy_range_byte($array, 0, 7);
      unless (Array->equals_byte($range, [(byte)5, -7, 9, Fn->BYTE_MIN(), 127, 15, 19])) {
        return 0;
      }
    }
    
    {
      my $array = [(byte)5, -7, 9, Fn->BYTE_MIN(), 127, 15, 19];
      my $range = Array->copy_range_byte($array, 1, 3);
      unless (Array->equals_byte($range, [(byte)-7, 9, Fn->BYTE_MIN()])) {
        return 0;
      }
    }
    
    # Exception
    {
      {
        eval { Array->copy_range_byte(undef, 0, 7); };
        unless (Fn->contains($@, "The array must be defined")) {
          return 0;
        }
      }
      
      {
        eval { Array->copy_range_byte([(byte)5, -7, 9], -1, 1); };
        unless (Fn->contains($@, "The offset must be greater than or equal to 0")) {
          return 0;
        }
      }
      
      {
        eval { Array->copy_range_byte([(byte)5, -7, 9], 0, -1); };
        unless (Fn->contains($@, "The length must be greater than or equal to 0")) {
          return 0;
        }
      }
      
      {
        eval { Array->copy_range_byte([(byte)5, -7, 9], 2, 2); };
        unless (Fn->contains($@, "The offset + the length specified by the argument must be less than or equal to the length of the array")) {
          return 0;
        }
      }
    }
    
    return 1;
  }
