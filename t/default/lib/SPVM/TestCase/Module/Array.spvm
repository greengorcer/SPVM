  static method copy_string : int () {
    # Copy array
    {
      my $value0 = "abc";
      my $array = [$value0, "def", "hij"];
      my $array_out = Array->copy_string($array);

      unless ($value0 != $array_out->[0]) {
        return 0;
      }
      unless ($array->[0] eq $array_out->[0]) {
        return 0;
      }
      unless ($array->[1] eq $array_out->[1]) {
        return 0;
      }
      unless ($array->[2] eq $array_out->[2]) {
        return 0;
      }

      unless ($array->[0] != $array_out->[0]) {
        return 0;
      }

      unless ($array->[1] != $array_out->[1]) {
        return 0;
