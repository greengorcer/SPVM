  static method copy_byte : int () {
    # Copy array
    {
      my $array = [(byte)1, 2, Fn->BYTE_MIN()];
      my $array_out = Array->copy_byte($array);
      
      unless ($array isa byte[]) {
        return 0;
      }
      unless ($array->[0] == 1) {
        return 0;
      }
      
      unless ($array->[1] == 2) {
        return 0;
      }
      
      unless ($array->[2] == Fn->BYTE_MIN()) {
        return 0;
      }
      
      unless (@$array_out == 3) {
        return 0;
      }
    }
    
    # Array is undef
    {
      my $output = Array->copy_byte(undef);
      unless ($output == undef) {
        return 0;
      }
    }
    
    return 1;
  }
  
